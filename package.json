{
  "name": "playwright_pet_project",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "generateReport": "node -e 'require(\"./createReportHelper.js\").createTestReport()'\n",
    "specViaPathToFile": "cross-env SUITE_TYPE=specFile wdio wdio.conf.js --spec tests/regression/accountSettings/admin/users/admin_users_settings.test.ts",
    "tests_ui": "npx playwright test --ui --trace on",
    "tests": "npx playwright test",
    "tests_headed": "npx playwright test --headed",
    "tests_debug": "npx playwright test --debug",
    "tests:activities": "cross-env SUITE_TYPE=folder wdio wdio.conf.js --suite activities",
    "tests:smoke": "cross-env SUITE_TYPE=folder SUITE=Smoke wdio wdio.conf.js --suite smoke",
    "specificTests": "wdio wdio.conf.js",
    "specificTests:somke": "cross-env SUITE_TYPE=specFile SUITE=smoke wdio wdio.conf.js",
    "openReport": "allure generate --clean ./test-results/allure-results -o ./test-results/allure-report && allure open ./test-results/allure-report",
    "findMissingIds": "node helpers/scenariosHelper.js findMissingIds",
    "getCountOfScenarios": "node helpers/scenariosHelper.js getCountOfScenarios",
    "findEmptySteps": "node helpers/scenariosHelper.js findEmptySteps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.3.1"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "path-exists": "^4.0.0",
    "prettier": "^2.0.5"
  },
  "directories": {
    "test": "tests"
  },
  "description": ""
}
